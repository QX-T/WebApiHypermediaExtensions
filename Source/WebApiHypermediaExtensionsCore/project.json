{
    "title": "Web Api Hypermedia Extensions",
    "description": "Web Api extensions to create restful Web services using Siren as Hypermedia format",
    "authors": [ "Mathias Reichardt" ],
    "packOptions": {
        "tags": [ "WebApi", "REST", "Siren", "Hypermedia" ],
        "projectUrl": "https://github.com/bluehands/WebApiHypermediaExtensions",
        "licenseUrl": "https://github.com/bluehands/WebApiHypermediaExtensions/blob/master/LICENSE.md",
        "files": {
            "mappings": {
                "lib/netstandard1.6/": {
                    "includeFiles": [
                        "bin/Release/netstandard1.6/Hypermedia.Util.dll",
                        "bin/Release/netstandard1.6/Hypermedia.Util.xml"
                    ]
                },
                "lib/net452/": {
                    "includeFiles": [
                        "bin/Release/net452/Hypermedia.Util.dll",
                        "bin/Release/net452/Hypermedia.Util.xml"
                    ]
                }
            }
        }
    },
    "version": "1.3.0-*",
    "dependencies": {
        "Hypermedia.Util": {
            "target": "project",
            "type": "build"

        },
        "Microsoft.AspNetCore.Mvc": "1.0.0",
        "NETStandard.Library": "1.6.0"
    },
    "frameworks": {
        "netstandard1.6": {
        },
        "net452": {
            "frameworkAssemblies": {
                "System.Runtime.Serialization": "4.0.0.0"
            }
        }
    },
    "buildOptions": {
        "xmlDoc": true,
        "nowarn": [ "1591" ]
    }
}
